{
  "name": "@theguild/components",
  "version": "4.5.9",
  "repository": {
    "url": "https://github.com/the-guild-org/docs",
    "directory": "packages/components"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": {
    "./style.css": "./style.css",
    "./search-bar-v2.css": "./search-bar-v2.css",
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./next.config": {
      "import": "./dist/next.config.mjs",
      "types": "./dist/next.config.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "search-bar-v2.css",
    "style.css"
  ],
  "scripts": {
    "build": "tsup",
    "prebuild": "pnpm types:check && rm -rf dist",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "^12.3.1 || ^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "1.8.3",
    "@algolia/autocomplete-plugin-algolia-insights": "1.8.3",
    "@algolia/autocomplete-plugin-query-suggestions": "1.8.3",
    "@algolia/autocomplete-theme-classic": "1.8.3",
    "@next/bundle-analyzer": "13.2.4",
    "@radix-ui/react-navigation-menu": "1.1.2",
    "algoliasearch": "4.16.0",
    "clsx": "1.2.1",
    "focus-trap-react": "10.1.1",
    "mermaid": "10.0.2",
    "next-videos": "1.5.0",
    "nextra": "2.3.0",
    "nextra-theme-docs": "2.3.0",
    "react-instantsearch-dom": "6.39.1",
    "react-player": "2.12.0",
    "remark-mdx-disable-explicit-jsx": "0.1.0",
    "search-insights": "2.4.0",
    "semver": "^7.3.8",
    "unist-util-visit": "4.1.2",
    "use-debounce": "9.0.3"
  },
  "devDependencies": {
    "@theguild/algolia": "workspace:*",
    "@types/mdast": "3.0.11",
    "@types/mermaid": "9.1.0",
    "@types/react-dom": "18.0.11",
    "@types/semver": "7.3.13",
    "esbuild-plugin-svgr": "1.1.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.7",
    "unified": "10.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}